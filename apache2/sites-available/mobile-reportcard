<VirtualHost *:80>
	ServerName mobile-reportcard.wmflabs.org
	ServerAdmin dandreescu@wikimedia.org
	
	DocumentRoot /srv/mobile-reportcard.wmflabs.org/limn/dist
	<Directory /srv/mobile-reportcard.wmflabs.org/limn/dist>
		Options Indexes FollowSymLinks MultiViews
		AllowOverride None
		Order allow,deny
		Allow from all
	</Directory>
	
	ServerSignature On
	CustomLog /var/log/apache2/limn-access.log combined
	ErrorLog /var/log/apache2/limn-error.log
	LogLevel info
	
	ProxyRequests On
	ProxyPreserveHost On
	ProxyPass / http://localhost:8089/ 
	ProxyPassReverse / http://localhost:8089/
	
	<Proxy *>
		Order allow,deny
		Allow from all
	</Proxy>
	
</VirtualHost>

